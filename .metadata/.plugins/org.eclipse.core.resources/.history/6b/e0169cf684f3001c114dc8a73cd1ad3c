// 싱글톤
public class GetMessageListService {
	
	private static GetMessageListService instance = null;

	private GetMessageListService() {}

	public static GetMessageListService getInstance() {
		if( instance == null ) {
			instance = new GetMessageListService();
		}
		return instance;
	}

}
